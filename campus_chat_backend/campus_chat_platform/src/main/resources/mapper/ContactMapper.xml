<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ccj.campus.chat.mapper.ContactMapper">

    <select id="getDisturbContact" resultType="java.lang.Long">
        SELECT uid
        from contact
        WHERE room_id = #{roomId}
          and not_disturb = 1
    </select>
</mapper>
